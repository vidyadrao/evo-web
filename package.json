  "main": "index.js",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "npm run build -w packages/skin && npm run build -w packages/ebayui-core && npm run build -w packages/ebayui-core-react",
    "build:ci": "CI=true npm run build -w packages/skin && npm run build -w packages/ebayui-core && npm run build -w packages/ebayui-core-react",
    "change": "changeset add",
    "copy:coreui-site": "npm run deploy --ws",
    "copy:skin-site": "npm run deploy --ws",
    "deploy": "npm run deploy --ws && BASE_URL=/evo-web/ marko-run build -o ./_site && npm run deploy:copy-site",
    "deploy:copy-site": "ncp packages/skin/_site/public _site/public/skin && ncp packages/ebayui-core/_site _site/public/ebayui-core && ncp packages/ebayui-core-react/_site _site/public/ebayui-core-react",
    "postinstall": "npx playwright install --with-deps chromium",
    "prepare": "husky",
    "release": "npm run release --ws && changeset publish",
    "test": "npm run test --ws",
    "update-snapshots": "npm run update-snapshots --ws --if-present",
    "version": "npm run version --ws && changeset version && npm i --package-lock-only && git add -A packages"
  },
  "commitlint": {
    "extends": "@commitlint/config-conventional"
  },
  "lint-staged": {
    "*.{js,css,md,less,json}": "prettier --write --with-node-modules",
    "*.{css,scss}": "stylelint --config .stylelintrc"
  },
  "browserslist": [
    "extends @ebay/browserslist-config"
  ],
